<script>
head.ready(document, function () {
    if (!window.jQuery) {
    	var jqueryUrl = "\/webjars\/jquery\/3.3.1\/jquery.min.js"; 
        head.load(jqueryUrl, loadjQueryUI);
    } else {
        notifyResourcesAreLoaded(resourceLoadedSuccessfully);
    }
});

function loadjQueryUI() {
	var jqueryUrl = "\/webjars\/jquery-ui\/1.12.1\/jquery-ui.min.js"; 
	head.load(jqueryUrl, loadjQueryCookies);
}

function loadjQueryCookies() {
	var jqueryUrl = "\/webjars\/jquery-cookie\/1.4.1-1\/jquery.cookie.js"; 
	head.load(jqueryUrl, notifyResourcesAreLoaded(resourceLoadedSuccessfully));
}

function notifyResourcesAreLoaded(callback) {
    if (typeof callback === "function") {
        callback();
    }
}

function refresh(){
    var timestamp = (new Date()).getTime();
    var url = "/ioias/portal/um/v19.2.0/user/tattedcode?date="+timestamp;
    $("#verifyCodeImage").attr("src", url);
}

$(function(){
        refresh();
        $('#resetPasswordButton').attr("disabled", true);
    });

function clearResetForm(){
    $('#userName').val("");
    $('#type').val("");
    $('#verifyCode').val("");
}

function checkForm(){
    var username = $('#userName').val();
    var type = $('#type').val();
    var verifyCode = $('#verifyCode').val();
    if(username != "" && type != "" && verifyCode != "" && verifyCode.length == 4){
        $('#resetPasswordButton').attr("disabled", false)
    }else(
        $('#resetPasswordButton').attr("disabled", true)
    )
}

function resetPassword() {
    var userName = $('#userName').val();
    var type = $('#type').val();
    var verifyCode = $('#verifyCode').val();
    var data = {userName: userName, type: type};
    $.ajax({
       type : "put",
       url : "/ioias/portal/um/v1.1.0/user/password/reset?verifyCode="+verifyCode,
       async : true,
       contentType: "application/json;charset-UTF-8",
       data : JSON.stringify(data),
       success : function(data,textStatus,xhr){
           if( xhr.status == 200 ){
               clearResetForm();
               $('#restPasswordMessage').css("color","white");
               $('#restPasswordMessage').html(data.resp);
               refresh();
           }else{
               clearResetForm();
               $('#restPasswordMessage').css("color","red");
               $('#restPasswordMessage').html(data.resp);
               refresh();
           }
       },  error: function(XMLHttpRequest, textStatus, xhr){
               var error = $.parseJSON(XMLHttpRequest.responseText);
               clearResetForm();
               $('#restPasswordMessage').css("color","red");
               $('#restPasswordMessage').html(error.error_desc);
               refresh();
       },
       dataType : "json"
    });
}

</script>

